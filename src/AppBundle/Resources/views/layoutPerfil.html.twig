{% extends '::base.html.twig' %}

{% block title %}

{% endblock %}

{% block stylesheets %}

    <link href="{{ asset('bundles/estilos/css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/estilos/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/estilos/css/shop-homepage.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/estilos/css/font-awesome.min.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/estilos/css/sb-admin.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/estilos/css/morris.css') }}" type="text/css" rel="stylesheet" />
    <link href="{{ asset('bundles/estilos/css/estilo.css') }}" type="text/css" rel="stylesheet" />

{% endblock %}

{% block body %}

    {% block nav %}

    {% endblock %}

    <div class="container">
        <div class="row">

            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >

                {% block panel %}
                {% endblock %}

                    <div class="panel-heading">
                        {% block titulo %}
                        {% endblock %}
                    </div><!--/panel-heading-->

                    <div class="panel-body">
                        <div class="row">

                            {% block img %}
                            {% endblock %}

                            <div class=" col-md-9 col-lg-9 ">
                                <table class="table table-user-information">

                                    {% block table %}
                                    {% endblock %}

                                </table>
                            </div><!--/col-md-9-->

                        </div><!--/row-->
                    </div><!--/panel-body-->

                    <div class="panel-footer">
                        <div class="row">
                            {% block menssage %}
                                {% include 'AppBundle:Comunes:flashMessage.html.twig' %}
                            {% endblock %}
                        </div>
                    </div>

                </div><!--/panel panel-info-->

            </div><!--/col-xs-12-->

        </div><!--/row-->
    </div><!--/container-->

    {% include 'AppBundle:Comunes:footer.html.twig' %}

{% endblock %}

    {% block javascripts %}

        <!-- jQuery -->
        <script src="{{ asset('bundles/estilos/js/jquery.js') }}"></script>
        <!-- Bootstrap Core JavaScript -->
        <script src="{{ asset('bundles/estilos/js/bootstrap.min.js') }}"></script>
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

    {% endblock %}